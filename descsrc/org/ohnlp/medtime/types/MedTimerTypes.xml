<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
	<name>MedTime TypeSystem</name>
	<description>This is the Type System for MedTime</description>
	<version>1.0</version>
	<vendor>Mayo - NLP</vendor>
	<imports>
		<import name="org.ohnlp.types.TypeSystem"/>
	</imports>
	<types>
		<typeDescription>
			<name>org.ohnlp.medtime.type.MedTimex3</name>
			<description/>
			<supertypeName>uima.tcas.Annotation</supertypeName>
			<features>
				<featureDescription>
					<name>filename</name>
					<description/>
					<rangeTypeName>uima.cas.String</rangeTypeName>
				</featureDescription>
				<featureDescription>
					<name>firstTokId</name>
					<description/>
					<rangeTypeName>uima.cas.Integer</rangeTypeName>
				</featureDescription>
				<featureDescription>
					<name>allTokIds</name>
					<description/>
					<rangeTypeName>uima.cas.String</rangeTypeName>
				</featureDescription>
				<featureDescription>
					<name>timexId</name>
					<description/>
					<rangeTypeName>uima.cas.String</rangeTypeName>
				</featureDescription>
				<featureDescription>
					<name>timexInstance</name>
					<description/>
					<rangeTypeName>uima.cas.Integer</rangeTypeName>
				</featureDescription>
				<featureDescription>
					<name>contextSentence</name>
					<description/>
					<rangeTypeName>org.ohnlp.typesystem.type.textspan.Sentence</rangeTypeName>
				</featureDescription>
				<featureDescription>
					<name>timexType</name>
					<description/>
					<rangeTypeName>uima.cas.String</rangeTypeName>
				</featureDescription>
				<featureDescription>
					<name>timexValue</name>
					<description/>
					<rangeTypeName>uima.cas.String</rangeTypeName>
				</featureDescription>
				<featureDescription>
					<name>foundByRule</name>
					<description/>
					<rangeTypeName>uima.cas.String</rangeTypeName>
				</featureDescription>
				<featureDescription>
					<name>timexQuant</name>
					<description/>
					<rangeTypeName>uima.cas.String</rangeTypeName>
				</featureDescription>
				<featureDescription>
					<name>timexFreq</name>
					<description/>
					<rangeTypeName>uima.cas.String</rangeTypeName>
				</featureDescription>
				<featureDescription>
					<name>timexMod</name>
					<description/>
					<rangeTypeName>uima.cas.String</rangeTypeName>
				</featureDescription>
			</features>
		</typeDescription>
		 

		<typeDescription>
			<name>org.ohnlp.medtime.type.DocumentMetadata</name>
			<description>Structured data that captures information about the
				document, patient, or context of the clinical text.</description>
			<supertypeName>uima.tcas.Annotation</supertypeName>
			<features>
				<featureDescription>
					<name>fileName</name>
					<description/>
					<rangeTypeName>uima.cas.String</rangeTypeName>
				</featureDescription>
				<featureDescription>
					<name>timexId</name>
					<description/>
					<rangeTypeName>uima.cas.String</rangeTypeName>
				</featureDescription>
				<featureDescription>
					<name>timexValue</name>
					<description/>
					<rangeTypeName>uima.cas.String</rangeTypeName>
				</featureDescription>
			</features>
		</typeDescription>
	<typeDescription>
      <name>org.ohnlp.medtime.type.TimeInterval</name>
      <description>description of TimeInterval Type</description>
      <supertypeName>org.ohnlp.medtime.type.MedTimex3</supertypeName>
    <features>
        <featureDescription>
          <name>startTime</name>
          <description/>
          <rangeTypeName>org.ohnlp.medtime.type.TimeInstant</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>endTime</name>
          <description/>
          <rangeTypeName>org.ohnlp.medtime.type.TimeInstant</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>endDuration</name>
          <description/>
          <rangeTypeName>org.ohnlp.medtime.type.Duration</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>durationValue</name>
          <description/>
          <rangeTypeName>uima.cas.Float</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>durationUnit</name>
          <description/>
          <rangeTypeName>org.ohnlp.medtime.type.Granularity</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>org.ohnlp.medtime.type.TimeInstant</name>
      <description>TODO: The desciption of TimeInstant</description>
      <supertypeName>org.ohnlp.medtime.type.MedTimex3</supertypeName>
    <features>
        <featureDescription>
          <name>granularity</name>
          <description>description of granularity</description>
          <rangeTypeName>org.ohnlp.medtime.type.Granularity</rangeTypeName>
        </featureDescription>
      <featureDescription>
          <name>originTime</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      <featureDescription>
          <name>hasNormalizedTime</name>
          <description/>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>hasOriginTime</name>
          <description/>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>hasGranularity</name>
          <description/>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>hasApproximation</name>
          <description>should be true by default</description>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>org.ohnlp.medtime.type.Granularity</name>
      <description>description of Granularity</description>
      <supertypeName>uima.cas.String</supertypeName>
      			<allowedValues>
				<value>
          <string>day</string>
          <description/>
        </value>
        <value>
          <string>year</string>
          <description/>
        </value>
        <value>
          <string>month</string>
          <description/>
        </value>
      <value>
          <string>second</string>
          <description/>
        </value>
        <value>
          <string>minute</string>
          <description/>
        </value>
        <value>
          <string>hour</string>
          <description/>
        </value>
        <value>
            <string>week</string>
            <description/>
        </value>
      </allowedValues>
    </typeDescription>
  <typeDescription>
      <name>org.ohnlp.medtime.type.Duration</name>
      <description/>
      <supertypeName>org.ohnlp.medtime.type.MedTimex3</supertypeName>
      <features>
        <featureDescription>
          <name>durationValue</name>
          <description/>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>durationUnit</name>
          <description/>
          <rangeTypeName>org.ohnlp.medtime.type.Granularity</rangeTypeName>
        </featureDescription>
      <featureDescription>
          <name>hasDurationUnit</name>
          <description/>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>hasDurationValue</name>
          <description/>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>hasOriginForm</name>
          <description/>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>org.ohnlp.medtime.type.TimePhase</name>
      <description/>
      <supertypeName>org.ohnlp.medtime.type.TimeInterval</supertypeName>
    </typeDescription>
  </types>
</typeSystemDescription>
