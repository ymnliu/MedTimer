        </featureDescription>
        <featureDescription>
          <name>valueType</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>labSentence</name>
          <description/>
          <rangeTypeName>org.ohnlp.typesystem.type.textsem.LabLinkerSentence</rangeTypeName>
        </featureDescription>
      </features>
