      <name>org.ohnlp.typesystem.type.textsem.LabLinkerSentence</name>
      <description>Sentence with LabLinker info</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>annotationText</name>
          <description>annotated LabLinker sentence fro raw text</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      <featureDescription>
          <name>orgSentence</name>
          <description/>
          <rangeTypeName>org.ohnlp.typesystem.type.textspan.Sentence</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
      <typeDescription>
      <name>org.ohnlp.medtime.type.LabValueMention</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>labTestName</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>labTestValue</name>
          <description/>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>labTestMin</name>
          <description/>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>labTestMax</name>
          <description/>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
      <featureDescription>
          <name>time</name>
          <description/>
          <rangeTypeName>org.ohnlp.medtime.type.MedTimex3</rangeTypeName>
        </featureDescription>
